        "development": "tsx watch src/server.ts",
        "build": "rimraf dist && tsc -p tsconfig.json",
        "start": "node dist/src/server.js",
        "test": "NODE_ENV=test npx jest",
        "test:integration": "NODE_ENV=test jest --testPathPatterns=test/integration-test",
        "test:unit": "NODE_ENV=test jest --testPathPatterns=test/unit-test",
        "knex": "node -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile knexfile.ts",
        "knex:create": "npm run knex -- migrate:make",
        "knex:migrate": "npm run knex -- migrate:latest",
        "knex:seed": "npm run knex -- seed:run"
    },
